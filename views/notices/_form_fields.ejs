<div class="form-group" style="padding-bottom: 10px;">
    <label for="title">Title</label>
    <input class="form-control" type="text" name="title" id="title" required value="<%= notice.title %>">
    
</div>
<div class="form-group" style="padding-bottom: 10px;">
    <label for="Description">Description</label>
    <textarea class="form-control" name="Description" required id="Description"><%= notice.description %></textarea>
    
</div>
<div class="form-group" style="padding-bottom: 10px;">
    <label for="Markdown">Markdown</label>
    <textarea class="form-control" name="Markdown" required id="Markdown"><%= notice.markdown %></textarea>
    
</div>
<a href="/home" class="btn btn-secondary">Cancel</a>
<button type="submit" class="btn btn-primary">Save</button>